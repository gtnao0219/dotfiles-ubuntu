[core]
  excludesfile = ~/.gitignore_global

[user]
  name  = naotaka nakane
  email = n.nakane0219@gmail.com

[ui]
  ui = auto

[ghq]
  root = ~/dev/src

[alias]
  st = status
  sts = status -s
  ft = fetch
  ftp = fetch --prune
  ad = add
  c = commit
  ci = commit -a
  co = checkout
  cob = checkout -b
  br = branch
  bra = branch -a
  sw = switch
  swc = switch -c
  df = diff
  dfs = diff --staged

